      "source": [
        "n= int(input(\"enter any whole number : \"))\n",
        "fac= 1\n",
        "if(n==0):\n",
        "  print(\" the factorial of the given number is 1\")\n",
        "else:\n",
        "  for i in range(1,n+1,1):\n",
        "    fac=fac*i\n",
        "  print(\" the factorial of the given number is \",fac)\n"
      ]
    }
  ]
